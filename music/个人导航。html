<script>
  function bofang(){
    var audio111 = $('#audi0111');
        console.log(audio111)
         var promise =  audio111[0].play();
            if (promise !== undefined) {
            promise.then(_ => {
                // Autoplay started!
            }).catch(error => {
              console.log(error)
                // Autoplay was prevented.
                // Show a "Play" button so that user can start playback.
            });
        }
  }
  var bo = 0

  window.onload = function () {
        bofang()
      $(document).click(function () {
        if (bo === 0) {
                  bofang()
          bo = 1
        }

      })


    }
</script>

